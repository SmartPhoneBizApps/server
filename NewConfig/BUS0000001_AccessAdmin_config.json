    "style": "SAP",
    "calculation": "No",
    "USP": "UserProfile",
    "StatusColor": "Yes",
    "SearchString": { "Search": false },
    "Filters": [
      {
        "$and": [
          { "Status": { "$ne": "Resolved" } },
          { "Status": { "$ne": "MoreInfo" } },
          { "Status": { "$ne": "Saved" } },
          { "Status": { "$ne": "Cancelled" } }
        ]
      },
      {
        "$or": [
          { "IssueType": "Missing Authorizations" },
          { "IssueType": "Application Access" },
          { "IssueType": "I am unable to log in" },
          { "IssueType": "Password Reset" }
        ]
      }
    ],
    "Partner": "@user",
    "UserSpecific": "Yes",
    "Update": "Yes",
    "URL": "https://fierce-oasis-51455.herokuapp.com",
    "Source": { "SourceName": "mongoDB", "SourceFile": "NA" },

    "filterFields": {
      "header": ["Status", "Department", "DateCreated", "DateResolved", "Score"]
    }
  },
  "ListFields": {
    "IntroField": "IssueType",
    "TitleField": "DescribeProblem",
