{
  "Title": {
    "ApplicationTitle": "Approve Shopping Cart",
    "DetailTitle": "Approve Shopping Cart"
  },
  "Constants": {},
  "Controls": {
    "style": "SAP",
    "calculation": "Yes",
    "defaultGraph": "StackedBar",
    "USP": "UserProfile",
    "StatusColor": "Yes",
    "SearchString": { "Search": false },
    "Filters": [
      {
        "$and": [
          { "Status": { "$ne": "Approved" } },
          { "Status": { "$ne": "Rejected" } },
          { "Status": { "$ne": "ReferredBack" } },
          { "Status": { "$ne": "Saved" } },
          { "Status": { "$ne": "Cancelled" } }
        ]
      }
    ],
    "Partner": "@user",
    "UserSpecific": "Yes",
    "Update": "Yes",
    "URL": "https://fierce-oasis-51455.herokuapp.com",
    "Source": { "SourceName": "mongoDB", "SourceFile": "NA" },
    "filterFields": {
      "header": [
        "Status",
        "VendorName",
        "CostCentre",
        "Currency",
        "VATRate",
        "GLAccount"
      ]
    }
  },
  "ListFields": {
    "IntroField": "ID",
    "TitleField": "VendorName",
    "fStatus": "Status",
    "numberField": "GrossValue",
    "LFields": ["SCDate", "DelTo", "PostCode", "Email", "DeliveryDt"],
    "numberUnitField": "Currency"
  },
  "ListBOTFields": {
    "None": ["cardImage", "StatusState"],
    "Title": ["VendorName"],
    "SubTitle": ["GrossValue", "Status", "DeliveryDt"],
    "Description": ["ID", "SCDate", "DelTo", "PostCode", "Email"]
