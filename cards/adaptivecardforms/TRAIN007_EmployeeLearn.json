{
  "body": [
    {
      "type": "TextBlock",
      "text": "Your Name",
      "isSubtle": true,
      "size": "medium"
    },
    {
      "type": "Input.Text",
      "placeholder": "Last, First",
      "style": "text",
      "id": "SimpleVal"
    },
    {
      "type": "TextBlock",
      "text": "Email address",
      "isSubtle": true,
      "size": "medium"
    },
    {
      "type": "Input.Text",
      "placeholder": "youremail@example.com",
      "style": "email",
      "id": "EmailVal"
    },
    {
      "type": "TextBlock",
      "text": "Phone Number",
      "isSubtle": true,
      "size": "medium"
    },
    {
      "type": "Input.Text",
      "placeholder": "xxx.xxx.xxxx",
      "style": "tel",
      "id": "TelVal",
      "maxLength": 10
    },
    {
      "type": "TextBlock",
      "text": "Age",
      "isSubtle": true,
      "size": "medium"
    },
    {
      "type": "Input.Number",
      "id": "NumVal",
      "min": 0,
      "max": 110,
      "placeholder": "Age"
    },
    {
      "type": "TextBlock",
      "text": "Starting Date",
      "isSubtle": true,
      "size": "medium"
    },
    {
      "type": "Input.Date",
      "placeholder": "Start Date",
      "id": "DateVal",
      "value": "2017-09-20"
    },
    {
      "type": "Input.Toggle",
      "title": "I accept the terms and conditions",
      "id": "Checked",
      "wrap": true,
      "value": "false",
      "valueOn": "true",
      "valueOff": "false"
    }
  ]
}
