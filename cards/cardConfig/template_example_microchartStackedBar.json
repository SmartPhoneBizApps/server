{
  "cardMinRows": 4,
  "cardColumn": 3,
  "sap.app": {
    "id": "smartphoneapps",
    "type": "card",
    "applicationVersion": {
      "version": "1.0.0"
    }
  },
  "sap.card": {
    "type": "List",
    "header": {
      "title": "@Title",
      "actions": [
        {
          "type": "Custom",
          "parameters": {
            "type": "tabNavigation",
            "tab": "@HeaderActionURL"
          }
        }
      ]
    },
    "content": {
      "data": {
        "json": [
          {
            "Name": 2017,
            "Category": "Computer system accessories",
            "Expected": 200,
            "Actual": 500
          },
          {
            "Name": 2018,
            "Category": "Computer system accessories",
            "Expected": 300,
            "Actual": 320
          }
        ]
      },
      "item": {
        "title": "Notebooks Distribution",
        "subTitle": "by Names",
        "chart": {
          "type": "StackedBar",
          "displayValue": "{= ${Expected} + ${Actual}}",
          "maxValue": "{/maxOverNames}",
          "bars": [
            {
              "value": "{Expected}",
              "displayValue": "{legend/items/Expected}: {Expected}",
              "legendTitle": "{legend/items/Expected}"
            },
            {
              "value": "{Actual}",
              "displayValue": "{legend/items/Actual}: {Actual}",
              "legendTitle": "{legend/items/Actual}"
            }
          ]
        }
      }
    }
  }
}
